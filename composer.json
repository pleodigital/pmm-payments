{
    "name": "pleodigital/pmm-payments",
    "description": "Payment processing plugin for the Polish Medical Mission",
    "type": "craft-plugin",
    "version": "1.0.17",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "pmm-payments"
    ],
    "support": {
        "docs": "https://pleodigital.com",
        "issues": "https://pleodigital.com"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pleo Digtial",
            "homepage": "https://pleodigital.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "league/csv": "^8.2",
        "ext-curl": "*",
        "openpayu/openpayu" : "2.2.*",
        "paypal/paypal-checkout-sdk": "^1.0.1"
    },
    "autoload": {
        "psr-4": {
          "pleodigital\\pmmpayments\\": "src/"
        }
    },
    "extra": {
        "name": "pmm-payments",
        "handle": "pmm-payments",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "???",
        "components": {
            "payments": "pleodigital\\pmmpayments\\services\\Payments"
        },
        "class": "pleodigital\\pmmpayments\\Pmmpayments"
    }
}
